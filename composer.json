{
    "name": "akbardwi/laratheme",
    "description": "Theme and asset management for laravel",
    "keywords": ["theme", "child-theme", "multi", "laratheme", "asset", "management", "laravel", "lumen"],
    "homepage": "https://github.com/akbardwi/laratheme",
    "license": "CC-BY-3.0",
    "authors": [
        {
            "name": "Akbar Dwi Syahputra",
            "email": "admin@mail.akbardwi.my.id"
        }
    ],
    "require": {
        "hassankhan/config": "^2.1",
        "symfony/yaml": "^5.2 || ^6.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.3",
        "phpunit/phpunit": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Akbardwi\\Laratheme\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Akbardwi\\Laratheme\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Akbardwi\\Laratheme\\Providers\\LarathemeServiceProvider"
            ],
            "aliases": {
                "Theme": "Akbardwi\\Laratheme\\Facades\\Theme"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "suggest": {
        "symfony/yaml": "Allows for parsing of YAML theme info and change log files"
    }
}
